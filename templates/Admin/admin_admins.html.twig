{% extends 'Admin/base.html.twig' %}

{% block main %}
    <a class="btn" href="{{ path('admin_insert_admin') }}">Creer Admin</a>
{% for admin in admins %}
    <article>
        <h2>{{ admin.email }}</h2>
        <div>
           {%for role in admin.roles %}
               <p>{{ role }}</p>
           {% endfor %}
        </div>
        <p>{{ admin.password }}</p>
    </article>
{% endfor %}
{% endblock %}





